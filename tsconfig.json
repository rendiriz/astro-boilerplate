{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/islands/*": ["src/islands/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/lib/*": ["src/lib/*"],
      "@/pages/*": ["src/pages/*"]
    }
  }
}
